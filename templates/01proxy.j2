// {{ ansible_managed }}
//

{% if apt__proxy is defined %}
{%   if apt__proxy['https']|d(False)|bool %}
{%     set proxy = "https://{{ apt__proxy['host'] }}:{{ apt__proxy['port']|d(3128) }}/" %}
{%   else %}
{%     set proxy = "http://{{ apt__proxy['host'] }}:{{ apt__proxy['port']|d(3128) }}/" %}
{%   endif %}
Acquire::https::Proxy "{{ proxy }}";
Acquire::http::Proxy "{{ proxy }}";
Acquire::ftp::Proxy "{{ proxy }}";
{% endif %}
